/* does .align n align to n */
#undef AS_HAS_MULTIPLE_ALIGN


/* Define if dlopen needs a leading underscore. */
#undef DLOPEN_UNDERSCORE

/* for virtual sites */
#undef key_t

/* type check */
#undef HAVE_SOCKLEN_T
#ifndef HAVE_SOCKLEN_T
typedef int socklen_t;
#endif

#undef HAVE_STDINT_H

/* whether gmp.h needs c++ */
